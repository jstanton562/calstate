{#
/**
 * @file
 * Template for the system menu block.
 */
#}
{% set classes = [
  'block',
  bundle ? 'block--' ~ bundle|clean_class,
  id ? 'block--' ~ id|replace({"_": "-"})|clean_class,
]|merge(utility_classes ? utility_classes : []) %}


 <nav{{ attributes|without('id').addClass(classes) }}>

  {{ title_prefix }}
  {% if label %}
    {% block label %}
      <h3{{ title_attributes.addClass('menu-heading') }}>{{ label }}</h3>
    {% endblock %}
  {% endif %}
  {{ title_suffix }}

  {% block content %}
    {{ content }}
  {% endblock %}

 </nav>

